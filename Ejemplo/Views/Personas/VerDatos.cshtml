@model Ejemplo.Models.ca_Personas

@{
    ViewBag.Title = "Datos";
    Layout = null;
}

<fieldset>
    <div class="container">
    <div id="header" class="texto_centrado"><label>DATOS DE LA PERSONA</label></div>
    <div class="row text-center">
    <div class="display-field">
        <h3>CURP: @Html.DisplayFor(model => model.curp)</h3>
    </div>
    <div class="display-field">
        <h3>Nombre completo: @Html.DisplayFor(model => model.nombre) @Html.DisplayFor(model => model.primer_Apellido) @Html.DisplayFor(model => model.segundo_Apellido)</h3>
    </div>
    <div class="display-label">
         <h3>Cédula profesional: @Html.DisplayFor(model => model.Cedula_Profesional)</h3>
    </div>
    <div class="display-field">
        <h3>Teléfono: @Html.DisplayFor(model => model.Telefono)</h3>
    </div>
    <div class="display-field">
        <h3>Fecha de registro: @Html.DisplayFor(model => model.fecha_Registro)</h3>
    </div>
    <div class="display-field">
        <h3>Alias: @Html.DisplayFor(model => model.alias)</h3>
    </div>
</fieldset>
<p>
</p>
